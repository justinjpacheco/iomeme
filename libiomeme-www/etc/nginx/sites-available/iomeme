upstream iomeme {
  server 127.0.0.1:8080;
}

server {

  listen 80;

  server_name *.iome.me;

  location / {
    proxy_read_timeout 300;
    proxy_pass http://iomeme;
    proxy_set_header Host $host;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header X-Forwarded-HTTPS 0;
  }

}
